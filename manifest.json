{
  "name": "Search Overflow",
  "description": "Search Stack Overflow answers right from DevTools",
  "author": "Moin Uddin",
  "manifest_version": 2,
  "version": "0.0.1",
  "minimum_chrome_version": "10.0",
  "devtools_page": "so.html",
  "icons": {
    "16": "assets/img/16.png",
    "32": "assets/img/32.png",
    "64": "assets/img/64.png",
    "128": "assets/img/128.png"
  },
  "permissions": [
    "<all_urls>",
    "unlimitedStorage"
  ]
}
